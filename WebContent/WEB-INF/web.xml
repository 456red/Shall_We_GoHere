<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">

  <servlet>
  	<servlet-name>member</servlet-name>
  	<servlet-class>com.gohere.control.MemberController</servlet-class>
  	<init-param>
  		<param-name>property</param-name>
  		<param-value>member.properties</param-value>
  	</init-param>
  </servlet>
  <servlet-mapping>
  	<servlet-name>member</servlet-name>
  	<url-pattern>*.member</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>email</servlet-name>
  	<servlet-class>com.gohere.control.EmailController</servlet-class>
  	<init-param>
  		<param-name>property</param-name>
  		<param-value>email.properties</param-value>
  	</init-param>
  </servlet>
  <servlet-mapping>
  	<servlet-name>email</servlet-name>
  	<url-pattern>*.email</url-pattern>
  </servlet-mapping>
</web-app>